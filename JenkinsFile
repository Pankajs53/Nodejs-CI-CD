pipeline{
    agent any
    stages{
        stage('Checkout'){
            steps{
                sh 'echo passed'
                // git branch: 'main', url: 'https://github.com/Pankajs53/Nodejs-CI-CD.git'
            }
        }
        stage('Build And Test'){
            steps{
                sh 'echo Installing Packages'
                sh 'ls -ltr'
                sh 'npm install'
                sh 'echo Package Installation Done'
            }
        }
    }
}
